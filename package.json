{
  "name": "Mooncraft",
  "version": "0.1.0",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "sass": "sass --watch --sourcemap=auto --load-path node_modules client/src/scss/app.scss:client/public/css/app-bundle.css",
    "server": "nodemon --watch server server/main.js",
    "workers": "nodemon --watch server server/workers/main.js",
    "dev": "parallelshell 'npm run webpack-dev-server' 'npm run sass' 'npm run server' 'npm run workers'"
  },
  "dependencies": {
    "bluebird": "^3.2.2",
    "chalk": "^1.1.1",
    "crypto-js": "^3.1.6",
    "express": "^4.13.3",
    "forever": "^0.15.1",
    "redis": "^2.4.2",
    "secure-filters": "^1.0.5",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "parallelshell": "^2.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
