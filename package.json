{
  "name": "Mooncraft",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "npm run dist",
    "webpack-dev-server": "webpack-dev-server",
    "server": "nodemon --watch server server/main.js",
    "workers": "nodemon --watch server server/workers/main.js",
    "packager": "nodemon --watch server/db/fixture server/db/fixture/packager.js",
    "dev": "parallelshell 'npm run webpack-dev-server' 'npm run server' 'npm run workers' 'npm run packager'",
    "dist": "node server/db/fixture/packager.js && webpack --config webpack.dist.config.js",
    "start": "parallelshell 'pm2 start server/main.js' 'pm2 start server/workers/main.js'"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "bluebird": "^3.2.2",
    "chalk": "^1.1.1",
    "crypto-js": "^3.1.6",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "howler": "^1.1.29",
    "http-auth": "^2.2.8",
    "immutable": "^3.7.6",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "pm2": "^1.0.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "redis": "^2.4.2",
    "redux": "^3.3.1",
    "sass-loader": "^3.2.0",
    "secure-filters": "^1.0.5",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.4.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "nodemon": "^1.9.1",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
